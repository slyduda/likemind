{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "migrate:generate": "drizzle-kit generate:pg",
    "migrate:push": "drizzle-kit push:pg",
    "studio": "drizzle-kit studio --port 5000 --verbose",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.6",
    "drizzle-valibot": "^0.2.0",
    "nuxt": "^3.11.1",
    "pg": "^8.11.4",
    "valibot": "^0.30.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.3.6",
    "@nuxt/test-utils": "^3.12.0",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vue/test-utils": "^2.4.5",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^14.7.1",
    "husky": "^9.0.11",
    "playwright-core": "^1.43.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo ”[Husky] pre-commit”"
    }
  }
}
